<mvc:View controllerName="fioriapp.controller.Master"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
>
    <Page id="MasterPage">
    <content>
        <List
            id="masterActionList"
            headerText="Actions"
            mode="SingleSelectMaster"
            itemPress="onItemPress"
        >
            
            <StandardListItem
                id="1"
                title="Add Employee"
                icon="sap-icon://add"
                type="Active"
             />

            <StandardListItem
                id="2"
                title="View All Employees"
                icon="sap-icon://group"
                type="Active"
            />
            <StandardListItem
                id="3"
                title="Default"
                icon="sap-icon://accessibility"
                type="Active"
            />

        </List>
    </content>
    </Page>
</mvc:View>